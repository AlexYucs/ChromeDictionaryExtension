{
  "manifest_version": 2,

  "name": "Word Bank",
  "description": "This will define words in tabs",
  "version": "1.1",

  "content_scripts": [
   {
     "matches": ["<all_urls>"],
     "js": ["content.js"]
   }
 ],
 
 "background": {
    "scripts": ["background.js"],
	"persistent": false
  },
  
 "permissions": [
    "contextMenus",
    "tabs"
	
 ]
 

}
